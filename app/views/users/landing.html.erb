<h1>Landing</h1>
<p>Find me in app/views/users/index.html.erb</p>
<p>
  <%= link_to 'Sign Up', new_user_path %>
</p>
<p id="alert"><%= alert %></p>
<p id="alert"><%= notice %></p>

<%= form_tag sessions_path do %>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="actions"><%= submit_tag "Login" %></div>
<% end %>

